{
  "rule_name": "linux-sudo",
  "rule_author": "siembol",
  "rule_version": 1,
  "source_type": "linux-syslog",
  "matchers": [
    {
      "is_enabled": true,
      "matcher_type": "REGEX_MATCH",
      "is_negated": false,
      "field": "linux-syslog",
      "data": "true"
    }
  ],
  "table_mapping": {
    "table_name": "sudo",
    "joining_key": "table",
    "enriching_fields": [
      {
        "table_field_name": "table_size",
        "event_field_name": "sudo_table_size"
      }
    ]
  }
}