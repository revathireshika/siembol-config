{
  "parser_name": "siem-demo",
  "parser_version": 1,
  "parser_author": "siembol",
  "parser_description": "siem demo parser!",
  "parser_attributes": {
    "parser_type": "syslog",
    "syslog_config": {
      "syslog_version": "RFC_3164, RFC_5424",
      "merge_sd_elements": false,
      "timezone": "UTC"
    }
  },
  "parser_extractors": [
    {
      "is_enabled": true,
      "name": "pattern",
      "field": "original_string",
      "extractor_type": "regex_select_extractor",
      "attributes": {
        "should_overwrite_fields": false,
        "should_remove_field": false,
        "remove_quotes": false,
        "skip_empty_values": false,
        "thrown_exception_on_error": false,
        "regex_select_config": {
          "output_field": "sudo",
          "patterns": [
            {
              "pattern": "sudo"
            }
          ]
        }
      }
    }
  ],
  "transformations": [
    {
      "is_enabled": true,
      "transformation_type": "delete_fields",
      "attributes": {
        "fields_filter": {
          "including_fields": [
            "HOSTNAME"
          ]
        }
      }
    }
  ]
}